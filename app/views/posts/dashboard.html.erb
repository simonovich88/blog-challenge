<h1>Hola Mundo-Dashboard</h1>
<table class="table">
<tr>
    <th>Titulo</th>
    <th>Fecha de Ingreso</th>
    <th>Imagen</th>
    <th>Contenido</th>
</tr>
  <% @posts.each do |post| %>
<tr>
    <td><%= post.title %> </td>
    <td><%= post.created_at %> </td>
    <td><%= post.image_url %> </td>
    <td><%= post.content %> </td>
</tr>
<% end %>
</table>